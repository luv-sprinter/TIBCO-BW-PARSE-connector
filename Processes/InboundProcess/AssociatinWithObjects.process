<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx3="http://www.tibco.com/schemas/TIBCO-BW-PARSE-connector/Resources/Schemas/Schema.xsd10" xmlns:pfx2="http://www.tibco.com/schemas/TIBCO-BW-PARSE-connector/Resources/Schemas/Schema.xsd3" xmlns:pfx="http://www.tibco.com/schemas/file_processess/Resources/Schemas/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/file_processess/Resources/Schemas/Schema.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/file_processess/Resources/Schemas/Schema.xsd" schemaLocation="/Resources/Schemas/Schema.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/TIBCO-BW-PARSE-connector/Resources/Schemas/Schema.xsd3" schemaLocation="/Resources/Schemas/PutData.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/TIBCO-BW-PARSE-connector/Resources/Schemas/Schema.xsd10" schemaLocation="/Resources/Schemas/AssociationWithObjects.xsd"/>
    <pd:name>Processes/InboundProcess/AssociatinWithObjects.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Classname" type="xsd:string"/>
                    <xsd:element ref="pfx3:root"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>46</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>1020</pd:endX>
    <pd:endY>64</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1382953832153</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>434</pd:x>
        <pd:y>186</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <fault>localname=JSONRestException namespace=http://schemas.tibco.com/bw/plugins/xml/5.0/RESTJSONExceptions</fault>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="ConvertXML-String">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>280</pd:x>
        <pd:y>64</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <textEncoding>UTF-8</textEncoding>
            <term ref="pfx3:root"/>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Start/root/pfx3:root"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="End_Logger">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>721</pd:x>
        <pd:y>64</pd:y>
        <config>
            <processName>/CLEP/Common_Logger.process</processName>
        </config>
        <pd:inputBindings>
            <event>
                <xsl:attribute name="timestamp">
                    <xsl:value-of select="current-dateTime()"/>
                </xsl:attribute>
                <xsl:attribute name="description">
                    <xsl:value-of select="&quot;end logging succesful&quot;"/>
                </xsl:attribute>
            </event>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Error_Logger">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>653</pd:x>
        <pd:y>186</pd:y>
        <config>
            <processName>/CLEP/Error_Logger.process</processName>
        </config>
        <pd:inputBindings>
            <event>
                <Description>
                    <xsl:value-of select="&quot;error logging successful&quot;"/>
                </Description>
                <time_stamp>
                    <xsl:value-of select="current-dateTime()"/>
                </time_stamp>
                <Stack_Trace>
                    <xsl:value-of select="$_error/ns1:ErrorReport/StackTrace"/>
                </Stack_Trace>
                <Error_Message>
                    <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                </Error_Message>
                <Message_Code>
                    <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                </Message_Code>
                <Process_Stack>
                    <xsl:value-of select="$_error/ns1:ErrorReport/ProcessStack"/>
                </Process_Stack>
            </event>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Invoke REST API">
        <pd:type>com.tibco.plugin.json.activities.RestActivity</pd:type>
        <pd:resourceType>ae.activities.RestActivity</pd:resourceType>
        <pd:x>597</pd:x>
        <pd:y>64</pd:y>
        <config>
            <restMethodUI>POST</restMethodUI>
            <restResponseType>JSON</restResponseType>
            <authChoiceUI>No Authentication</authChoiceUI>
            <restInputReferNode>
                <xsd:element name="Parameters">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Query" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Header" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Content-Type" type="xsd:string"/>
                                        <xsd:element name="X-Parse-REST-API-Key" type="xsd:string"/>
                                        <xsd:element name="X-Parse-Application-Id" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Body" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Form" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Text" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="type" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Binary" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="type" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content" type="xsd:base64Binary" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </restInputReferNode>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <URI>
                    <xsl:value-of select="concat(&quot;https://api.parse.com/1/classes/&quot;,$Start/root/Classname)"/>
                </URI>
                <ns:Parameters>
                    <Header>
                        <Content-Type>
                            <xsl:value-of select="&quot;application/json&quot;"/>
                        </Content-Type>
                        <X-Parse-REST-API-Key>
                            <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Parse/RestApiKey"/>
                        </X-Parse-REST-API-Key>
                        <X-Parse-Application-Id>
                            <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Parse/ApplicationID"/>
                        </X-Parse-Application-Id>
                    </Header>
                    <Body>
                        <Text>
                            <type>
                                <xsl:value-of select="&quot;json&quot;"/>
                            </type>
                            <content>
                                <xsl:value-of select="substring(substring($Render-JSON/ns:ActivityOutputClass/jsonString, 9), 1, string-length(substring($Render-JSON/ns:ActivityOutputClass/jsonString, 9))-1)"/>
                            </content>
                        </Text>
                    </Body>
                </ns:Parameters>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="RemoveNamespace">
        <pd:type>com.tibco.plugin.xml.XMLTransformActivity</pd:type>
        <pd:resourceType>ae.activities.XMLTransformActivity</pd:resourceType>
        <pd:x>394</pd:x>
        <pd:y>64</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <engine>TibcoXML</engine>
            <stylesheet>/Resources/XSLTfile/XSLT File.xslt</stylesheet>
        </config>
        <pd:inputBindings>
            <textInput>
                <xmlString>
                    <xsl:value-of xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$ConvertXML-String/xmlString"/>
                </xmlString>
            </textInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>483</pd:x>
        <pd:y>64</pd:y>
        <config>
            <SchemaType>generic</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <xmlString>
                    <xsl:value-of select="$RemoveNamespace/xmlString"/>
                </xmlString>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Start_Logger">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>156</pd:x>
        <pd:y>64</pd:y>
        <config>
            <processName>/CLEP/Common_Logger.process</processName>
        </config>
        <pd:inputBindings>
            <event>
                <xsl:attribute name="timestamp">
                    <xsl:value-of select="current-dateTime()"/>
                </xsl:attribute>
                <xsl:attribute name="description">
                    <xsl:value-of select="&quot;start logging successful&quot;"/>
                </xsl:attribute>
            </event>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Start_Logger</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Invoke REST API</pd:from>
        <pd:to>End_Logger</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>End_Logger</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Error_Logger</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Error_Logger</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start_Logger</pd:from>
        <pd:to>ConvertXML-String</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ConvertXML-String</pd:from>
        <pd:to>RemoveNamespace</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>RemoveNamespace</pd:from>
        <pd:to>Render JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render JSON</pd:from>
        <pd:to>Invoke REST API</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>