    </pd:activity>
    <pd:activity name="ConvertXML-String">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>290</pd:x>
        <pd:y>126</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <textEncoding>UTF-8</textEncoding>
            <term ref="pfx:root"/>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Start/pfx:root"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="End_Logger">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>841</pd:x>
        <pd:y>126</pd:y>
        <config>
            <processName>/CLEP/Common_Logger.process</processName>
        </config>
        <pd:inputBindings>
            <event>
                <xsl:attribute xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="timestamp">
                    <xsl:value-of select="current-dateTime()"/>
                </xsl:attribute>
                <xsl:attribute xmlns:xsl="http://www.w3.org/1999/XSL/Transform" name="description">
                    <xsl:value-of select="&quot;batch request worked successfully&quot;"/>
                </xsl:attribute>
            </event>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Error_Logger">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>952</pd:x>
        <pd:y>227</pd:y>
        <config>
            <processName>/CLEP/Error_Logger.process</processName>
        </config>
        <pd:inputBindings>
            <event>
                <Description>
                    <xsl:value-of select="&quot;error while processing batch request&quot;"/>
                </Description>
                <time_stamp>
                    <xsl:value-of select="current-dateTime()"/>
                </time_stamp>
                <Stack_Trace>
                    <xsl:value-of select="$_error/ns1:ErrorReport/StackTrace"/>
                </Stack_Trace>
                <Error_Message>
                    <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                </Error_Message>
                <Message_Code>
                    <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                </Message_Code>
                <Process_Stack>
                    <xsl:value-of select="$_error/ns1:ErrorReport/ProcessStack"/>
                </Process_Stack>
            </event>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Invoke REST API">
        <pd:type>com.tibco.plugin.json.activities.RestActivity</pd:type>
        <pd:resourceType>ae.activities.RestActivity</pd:resourceType>
        <pd:x>678</pd:x>
        <pd:y>126</pd:y>
        <config>
            <restMethodUI>POST</restMethodUI>
            <restResponseType>JSON</restResponseType>
            <authChoiceUI>No Authentication</authChoiceUI>
            <restInputReferNode>
                <xsd:element name="Parameters">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Query" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Header" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="X-Parse-Application-Id" type="xsd:string"/>
                                        <xsd:element name="X-Parse-REST-API-Key" type="xsd:string"/>
                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Body" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Form" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Text" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="type" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Binary" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="type" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content" type="xsd:base64Binary" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </restInputReferNode>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <URI>
                    <xsl:value-of select="&quot;https://api.parse.com/1/batch&quot;"/>
                </URI>
                <ns:Parameters>
                    <Header>
                        <X-Parse-Application-Id>
                            <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Parse/ApplicationID"/>
                        </X-Parse-Application-Id>
                        <X-Parse-REST-API-Key>
                            <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Parse/RestApiKey"/>
                        </X-Parse-REST-API-Key>
                        <Content-Type>
                            <xsl:value-of select="&quot;json&quot;"/>
                        </Content-Type>
                    </Header>
                    <Body>
                        <Text>
                            <type>
                                <xsl:value-of select="&quot;json&quot;"/>
                            </type>
                            <content>
                                <xsl:value-of select="'{ &quot;requests&quot;: [ { &quot;method&quot;: &quot;PUT&quot;,  &quot;path&quot;: &quot;/1/classes/checkDB/qagF5ARDq3&quot;, &quot;body&quot;: { &quot;message&quot;:&quot;play cricket&quot;}},{ &quot;method&quot;: &quot;DELETE&quot;, &quot;path&quot;: &quot;/1/classes/checkDB/tDdm6ZUkzK&quot;}]}'"/>
                            </content>
                        </Text>
                    </Body>
                </ns:Parameters>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="RemoveNamespace">
        <pd:type>com.tibco.plugin.xml.XMLTransformActivity</pd:type>
        <pd:resourceType>ae.activities.XMLTransformActivity</pd:resourceType>
        <pd:x>418</pd:x>
        <pd:y>126</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <engine>TibcoXML</engine>
            <stylesheet>/Resources/XSLTfile/XSLT File.xslt</stylesheet>
        </config>
        <pd:inputBindings>
            <textInput>
                <xmlString>
                    <xsl:value-of xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$ConvertXML-String/xmlString"/>
                </xmlString>
            </textInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
