<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>Processes/InboundProcess/ResetPassword.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="email" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>176</pd:startX>
    <pd:startY>61</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>818</pd:endX>
    <pd:endY>61</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1382950983080</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>462</pd:x>
        <pd:y>158</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <fault>localname=JSONRestException namespace=http://schemas.tibco.com/bw/plugins/xml/5.0/RESTJSONExceptions</fault>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="EndLogger">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>686</pd:x>
        <pd:y>61</pd:y>
        <config>
            <processName>/CLEP/Common_Logger.process</processName>
        </config>
        <pd:inputBindings>
            <event>
                <xsl:attribute name="timestamp">
                    <xsl:value-of select="current-dateTime()"/>
                </xsl:attribute>
                <xsl:attribute name="description">
                    <xsl:value-of select="&quot;Successfully completed reset password&quot;"/>
                </xsl:attribute>
            </event>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Error_Logger">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>644</pd:x>
        <pd:y>158</pd:y>
        <config>
            <processName>/CLEP/Error_Logger.process</processName>
        </config>
        <pd:inputBindings>
            <event>
                <Description>
                    <xsl:value-of select="&quot;Error Invoking Rest&quot;"/>
                </Description>
                <time_stamp>
                    <xsl:value-of select="current-dateTime()"/>
                </time_stamp>
                <Stack_Trace>
                    <xsl:value-of select="$_error/ns2:ErrorReport/StackTrace"/>
                </Stack_Trace>
                <Error_Message>
                    <xsl:value-of select="$_error/ns2:ErrorReport/Msg"/>
                </Error_Message>
                <Message_Code>
                    <xsl:value-of select="$_error/ns2:ErrorReport/MsgCode"/>
                </Message_Code>
                <Process_Stack>
                    <xsl:value-of select="$_error/ns2:ErrorReport/ProcessStack"/>
                </Process_Stack>
            </event>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Invoke REST API">
        <pd:type>com.tibco.plugin.json.activities.RestActivity</pd:type>
        <pd:resourceType>ae.activities.RestActivity</pd:resourceType>
        <pd:x>511</pd:x>
        <pd:y>61</pd:y>
        <config>
            <restMethodUI>POST</restMethodUI>
            <restResponseType>JSON</restResponseType>
            <authChoiceUI>No Authentication</authChoiceUI>
            <restInputReferNode>
                <xsd:element name="Parameters">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Query" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Header" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="X-Parse-Application-Id" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="X-Parse-REST-API-Key" type="xsd:string"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Body" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Form" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Text" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="type" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Binary" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="type" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content" type="xsd:base64Binary" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </restInputReferNode>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <URI>
                    <xsl:value-of select="&quot;https://api.parse.com/1/requestPasswordReset&quot;"/>
                </URI>
                <ns:Parameters>
                    <Header>
                        <X-Parse-Application-Id>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Parse/ApplicationID"/>
                        </X-Parse-Application-Id>
                        <X-Parse-REST-API-Key>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Parse/RestApiKey"/>
                        </X-Parse-REST-API-Key>
                    </Header>
                    <Body>
                        <Text>
                            <type>
                                <xsl:value-of select="&quot;json&quot;"/>
                            </type>
                            <content>
                                <xsl:value-of select="concat('{&quot;email&quot;:&quot;',$Start/root/email ,'&quot;}')"/>
                            </content>
                        </Text>
                    </Body>
                </ns:Parameters>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="StartLogger">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>318</pd:x>
        <pd:y>61</pd:y>
        <config>
            <processName>/CLEP/Common_Logger.process</processName>
        </config>
        <pd:inputBindings>
            <event>
                <xsl:attribute name="timestamp">
                    <xsl:value-of select="current-dateTime()"/>
                </xsl:attribute>
                <xsl:attribute name="description">
                    <xsl:value-of select="&quot;Successfully started Reset Password&quot;"/>
                </xsl:attribute>
            </event>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>StartLogger</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>EndLogger</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>StartLogger</pd:from>
        <pd:to>Invoke REST API</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Invoke REST API</pd:from>
        <pd:to>EndLogger</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Error_Logger</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Error_Logger</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>